{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"],
      "headers": {
        "x-vercel-serverless-handler": "index.handler",
        "x-vercel-serverless-function": "true",
        "x-vercel-cache": "server"
      },
      "continue": true,
      "status": 200,
      "timeout": 300
    }
  ]
}
