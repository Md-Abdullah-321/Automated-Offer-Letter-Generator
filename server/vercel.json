{
  "routes": [
    {
      "src": "/api/user/signin",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": "Origin, X-Requested-With, Content-Type, Accept",
      "allow": "https://your-frontend-domain.com, http://localhost:5173"
    },
    {
      "routes": [
        {
          "src": "/.*",
          "methods": ["GET", "POST", "PUT", "DELETE"],
          "headers": "Origin, X-Requested-With, Content-Type, Accept",
          "allow": "*"
        }
      ]
    }
  ]
}
